---
import ToolOpenGraph from "../../../components/ToolOpenGraph.astro";
import { tools } from "../../../constants/tools";
import BaseLayout from "../../../layouts/BaseLayout.astro";
import type { getStaticPaths } from "../index.astro";
import { Page } from "./_index";

export { getStaticPaths } from "../index.astro";

const { locale } = Astro.props;
---

<ToolOpenGraph tool={tools.performance} />

<BaseLayout {locale} noFooter>
  <Page client:only="react" {locale}>
    <Page skeleton slot="fallback" {locale} />
  </Page>
</BaseLayout>
