---
import ToolOpenGraph from "../../../components/ToolOpenGraph.astro";
import { tools } from "../../../constants/tools";
import BaseLayout from "../../../layouts/BaseLayout.astro";
import { Page } from "./_index";

export { getStaticPaths } from "../index.astro";

const { locale } = Astro.params;
---

<ToolOpenGraph tool={tools.mixer} />

<BaseLayout integrated>
  <Page {locale} client:only="react">
    <Page {locale} skeleton slot="fallback" />
  </Page>
</BaseLayout>
