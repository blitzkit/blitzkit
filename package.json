{
  "name": "blitzkit",
  "version": "1.20.4",
  "description": "ðŸŽ‰ Tools for everything World of Tanks Blitz",
  "main": "src/server.ts",
  "type": "module",
  "scripts": {
    "build:averages": "bun scripts/averages",
    "build:db": "prisma generate",
    "build:bot": "vite-node --script scripts/build --production",
    "build:website": "yarn build:db && yarn build:sitemap && next build",
    "build:sitemap": "vite-node --script scripts/sitemap",
    "build:docs": "vitepress build docs",
    "build:ratingleaderboard": "vite-node --script scripts/buildRatingLeaderboard",
    "build:assets": "vite-node --max_old_space_size=16384 --script scripts/buildAssets/index",
    "test": "vite-node --script scripts/test",
    "test2": "vite-node --script scripts/test2",
    "dev:bot": "vite-node --script scripts/build && yarn start:bot",
    "dev:docs": "vitepress dev docs",
    "dev:website": "next dev",
    "start:bot": "node --enable-source-maps dist/bot/index.cjs",
    "start:website": "next start",
    "host:docs": "vitepress preview docs",
    "publish:ratingleaderboard": "vite-node --script scripts/buildRatingLeaderboard --publish",
    "setup:patches": "vite-node --script scripts/applyPatches",
    "lint": "npx tsc",
    "lint:website": "next lint",
    "format": "prettier --write \"**/*.@(json|js|jsx|css|md|ts|tsx|html|xml|yml|gltf)\"",
    "lint:version": "vite-node --script scripts/checkVersionMissMatch",
    "knip": "knip",
    "update": "yarn upgrade --latest",
    "setup:depotdownloader": "vite-node --script scripts/setupDepotDownloader",
    "tunnel": "cloudflared tunnel --url http://localhost:3000",
    "discoverids": "scripts/discoverIds.ts"
  },
  "license": "MIT",
