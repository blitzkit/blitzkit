  "scripts": {
    "build:bot": "vite-node --script scripts/build",
    "build:website": "next build",
    "build:docs": "vitepress build docs",
    "build:ratingsleaderboard": "vite-node --script scripts/buildRatingsLeaderboard",
    "dev:bot": "yarn build:bot && yarn start:bot",
    "dev:docs": "vitepress dev docs",
    "dev:website": "next dev",
    "start:bot": "node --enable-source-maps --trace-warnings dist/bot/index.cjs",
    "start:website": "next start",
    "host:docs": "vitepress preview docs",
    "publish:ratingsleaderboard": "vite-node --script scripts/buildRatingsLeaderboard --publish",
    "zip": "zip -r dist.zip dist",
    "lint": "npx tsc",
    "lint:website": "next lint",
    "format": "prettier --write \"**/*.@(json|js|jsx|css|md|ts|tsx|html|xml|yml|gltf)\"",
    "knip": "knip",
    "update": "yarn upgrade --latest"
  },
  "license": "MIT",
